{
  "entities": [
    {
      "name": "loan",
      "description": "Loan entity representing various loan products",
      "tables": [
        "nondigital_adh433_v1",
        "outstanding_daily",
        "kb_adh433",
        "da_adh433"
      ],
      "attributes": [
        "order_id",
        "principal_outstanding",
        "final_outstanding",
        "leadger_balance",
        "order_type",
        "product_name",
        "region",
        "product_group"
      ]
    },
    {
      "name": "order",
      "description": "Order entity representing loan orders",
      "tables": [
        "outstanding_daily",
        "da_orders",
        "writeoff_users",
        "provisional_writeoff"
      ],
      "attributes": [
        "order_id",
        "order_type",
        "da_date"
      ],
      "relationships": [
        {
          "to": "loan",
          "via": "order_id"
        }
      ]
    },
    {
      "name": "product",
      "description": "Product entity representing loan products",
      "attributes": [
        "product_name",
        "product_group",
        "order_type"
      ],
      "categories": [
        "bank",
        "digital",
        "credit_card",
        "khatabook",
        "da"
      ]
    },
    {
      "name": "region",
      "description": "Geographic region entity",
      "values": ["OS", "NE"],
      "computation": "Based on branch_code and product_type"
    },
    {
      "name": "reportdate",
      "description": "Report date entity for data snapshots",
      "type": "date",
      "default_logic": "Varies by product type"
    }
  ]
}

